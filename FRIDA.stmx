	<model name="Wellbeing">
		<variables>
			<aux name="Normal reform delay">
				<eqn>30</eqn>
				<units>year</units>
			</aux>
			<aux name="Social trust effect\non reform delay">
				<eqn>1+&quot;sSTReoRD&lt;0&quot;*(Social_trust/Social_trust_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Social tension effect\non reform delay">
				<eqn>1+&quot;sSTEeoRD&gt;0&quot;*(Social_tension/Social_tension_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Satisfactory public\nspending">
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="Social tension">
				<eqn>1+&quot;sPPReoSTE&lt;0&quot;*(Observed_rate_of_progress-Acceptable_progress)*time_to_observe_progress</eqn>
				<units>1</units>
			</aux>
			<aux name="sPPReoSTE&lt;0">
				<doc>	Assume social tension = 1 at 0.02 1/y
		And social tension = 0.7 at 0.04</doc>
				<eqn>-15</eqn>
				<units>1</units>
			</aux>
			<aux name="Acceptable progress">
				<eqn>0.02</eqn>
				<units>1/year</units>
			</aux>
			<aux name="sSTEeoRD&gt;0">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="Social tension in 1980">
				<eqn>1.3</eqn>
				<units>1</units>
			</aux>
			<aux name="Acceptable inequality">
				<doc>	was 0.5</doc>
				<eqn>0.6</eqn>
				<units>1</units>
			</aux>
			<aux name="sSTReoRD&lt;0">
				<doc>	was -0.1</doc>
				<eqn>-1</eqn>
				<units>1</units>
			</aux>
			<aux name="Social trust">
				<eqn>SMTH1(Indicated_social_trust, Time_to_establish_social_trust, Social_trust_in_1980)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<aux name="Social trust in 1980">
				<eqn>0.6</eqn>
				<units>1</units>
			</aux>
			<aux name="Public spending effect\non social trust">
				<eqn>Public_spending_as_share_of_GDP/Satisfactory_public_spending</eqn>
				<gf>
					<yscale min="0" max="1"/>
					<xpts>0,1</xpts>
					<ypts>0,1</ypts>
				</gf>
				<units>1</units>
			</aux>
			<aux name="Public spending as share of GDP">
				<eqn>&quot;Public_spending_per_person_k$/p/y&quot;/&quot;GDP_per_person_k$/p/y&quot;</eqn>
				<units>1</units>
			</aux>
			<aux name="Time to establish\nsocial trust">
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<aux name="Min wellbeing from\nglobal warming">
				<doc>	was 0.15</doc>
				<eqn>0.2</eqn>
				<units>1</units>
			</aux>
			<aux name="sGWeoAW&lt;0">
				<doc>	was -1, so -0.6, so -0.5</doc>
				<eqn>-0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Threshold warming">
				<doc>	IPCC expects climate damage to be visible when warming passes 1 deg C and be significant at +2 deg C</doc>
				<eqn>1</eqn>
				<units>deg C</units>
			</aux>
			<aux name="Inequity effect on\nsocial trust">
				<eqn>&quot;Inequality_(1)&quot;/Acceptable_inequality</eqn>
				<gf>
					<yscale min="0" max="1"/>
					<xpts>0,1,2</xpts>
					<ypts>1,1,0</ypts>
				</gf>
				<units>1</units>
			</aux>
			<aux name="Indicated social trust">
				<eqn>Public_spending_effect_on_social_trust*Inequity_effect_on_social_trust</eqn>
				<units>1</units>
			</aux>
			<aux name="Diminishing return\npublic spending">
				<doc>	was 0.8, so 0.7
		More utility from public spending (0.8) than from dispoable income (0.5)</doc>
				<eqn>0.7</eqn>
				<units>1</units>
			</aux>
			<aux name="Threshold public\nspending">
				<doc>	Normal public spending at GDPpp=15.000 k$/p/y, namely 20 % of GDP</doc>
				<eqn>3</eqn>
				<units>k$/(p*year)</units>
			</aux>
			<aux name="Past AWI">
				<eqn>SMTH1(AVERAGE_WELLBEING_INDEX, Average_wellbeing_perception_delay, AWI_in_1980)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing\nperception delay">
				<doc>	was 4, 6, so 8, 10</doc>
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<aux name="Threshold disposable\nincome">
				<doc>	What is needed to satisfy the UN SDGs according to David Collste et al</doc>
				<eqn>15</eqn>
				<units>k$/(p*year)</units>
			</aux>
			<aux name="Threshold inequality">
				<doc>	Inequity index is defined as oso/wso = (1 - wso) / wso = 1/wso - 1 which equals 1 at wso=0.5, and</doc>
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="sIIeoAW&lt;0">
				<doc>	was -0.6, -1.2, so -1</doc>
				<eqn>-0.15</eqn>
				<units>1</units>
			</aux>
			<aux name="Diminishing return\ndisposable income">
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="AWI in 1980">
				<doc>	was 1, 1,02, so 0.98</doc>
				<eqn>0.65</eqn>
				<units>1</units>
			</aux>
			<aux name="Introduction period\nfor policy" access="output">
				<eqn>( IF &quot;Exogenous_introduction_period?&quot; &gt; 0 THEN Exogenous_introduction_period ELSE Reform_delay )</eqn>
				<units>year</units>
			</aux>
			<aux name="Average wellbeing\nfrom progress">
				<eqn>(1+&quot;sROPeoAW&gt;0&quot;*(Observed_rate_of_progress-Threshold_progress_rate))*Wellbeing_effect_of_participation*time_to_observe_progress</eqn>
				<units>1</units>
			</aux>
			<aux name="sROPeoAW&gt;0">
				<doc>	was 1, so 10, so 3, so 8, so 6, so 9</doc>
				<eqn>6</eqn>
				<units>1</units>
			</aux>
			<aux name="Observed rate of progress">
				<eqn>SMTH1(((AVERAGE_WELLBEING_INDEX-Past_AWI)/AVERAGE_WELLBEING_INDEX)/Average_wellbeing_perception_delay, Average_wellbeing_perception_delay, 0)</eqn>
				<isee:delay_aux/>
				<units>1/year</units>
			</aux>
			<aux name="Threshold progress rate">
				<doc>	If there is progress above 2 % per year, there is social clm</doc>
				<eqn>0.02</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Wellbeing effect of\nparticipation">
				<eqn>1+&quot;sLPeoAWP&gt;0&quot;*(&quot;Labour_participation_rate_(1)&quot;/Threshold_participation-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="AVERAGE WELLBEING INDEX">
				<eqn>(0.5*Average_wellbeing_from_disposable_income+0.5*Average_wellbeing_from_public_spending)*Average_wellbeing_from_inequality*Average_wellbeing_from_global_warming*Average_wellbeing_from_progress</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from\ndisposable income">
				<eqn>EXP(Diminishing_return_disposable_income+LN(&quot;Worker_disposable_income_k$/p/y&quot;/Threshold_disposable_income))</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from\npublic spending">
				<eqn>EXP(Diminishing_return_public_spending+LN(&quot;Public_spending_per_person_k$/p/y&quot;/Threshold_public_spending))</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing\nfrom inequality">
				<eqn>1+&quot;sIIeoAW&lt;0&quot;*(&quot;Inequality_(1)&quot;/Threshold_inequality-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Average wellbeing from\nglobal warming">
				<eqn>MAX(Min_wellbeing_from_global_warming, MIN(1, 1+&quot;sGWeoAW&lt;0&quot;*(Perceived_warming_deg_C/Threshold_warming-1)))</eqn>
				<units>1</units>
			</aux>
			<aux name="Exogenous introduction period?">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Exogenous introduction period">
				<eqn>30</eqn>
				<units>year</units>
			</aux>
			<aux name="Reform delay">
				<eqn>SMTH1(Indicated_reform_delay, Time_to_change_reform_delay)</eqn>
				<isee:delay_aux/>
				<units>year</units>
			</aux>
			<aux name="Indicated reform delay">
				<eqn>Normal_reform_delay*Social_trust_effect_on_reform_delay*Social_tension_effect_on_reform_delay</eqn>
				<units>year</units>
			</aux>
			<aux name="Time to change reform delay">
