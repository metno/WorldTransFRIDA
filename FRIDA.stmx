	<model name="Other_performance_indicators">
		<variables>
			<aux name="GDP per person k$/p/y" access="input">
				<units>k$/(p*year)</units>
			</aux>
			<aux name="UNIT G/(M*k)" access="input">
				<eqn>1</eqn>
				<units>G/(M*k)</units>
			</aux>
			<aux name="Population Mp" access="input">
				<units>Mp</units>
			</aux>
			<aux name="Population below 15">
				<eqn>(Population_Mp*Fraction_below_15)*&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Normal k">
				<doc>	was 1, so 0.3, so 0.15, so 0.2
		goal is to have 600 Mp &gt; 15k$/y in 2000, ie 10 % of population at the time. And 50 % &gt; 15 k$/p/y in 2020</doc>
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="sINEeoLOK&lt;0">
				<doc>	was - 1</doc>
				<eqn>-0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Inequity effect\non logistic k">
				<eqn>1+&quot;sINEeoLOK&lt;0&quot;*(&quot;Inequality_(1)&quot;/0.5-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Inequality (1)" access="input">
				<units>1</units>
			</aux>
			<aux name="Logistic k">
				<eqn>Normal_k*Inequity_effect_on_logistic_k</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction below 15">
				<eqn>1-(1/(1+EXP(-Logistic_k*(&quot;GDP_per_person_k$/p/y&quot;-14))))</eqn>
				<units>k$/p/year</units>
			</aux>
			<aux name="Cost of TAs" access="output">
				<eqn>Cost_of_food_and_energy_TAs</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of food and\nenergy TAs">
				<eqn>&quot;Cost_of_food_G$/y&quot;+&quot;Cost_of_energy_G$/y&quot;</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Cost of energy G$/y" access="input">
				<units>G$/year</units>
			</aux>
			<aux name="Cost of food G$/y" access="input">
				<units>G$/year</units>
			</aux>
