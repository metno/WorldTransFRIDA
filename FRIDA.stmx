	<model name="Public_sector">
		<variables>
			<aux name="Public spending\nper person" access="output">
				<eqn>(&quot;Govmnt_spending_G$/y&quot;/Population_Mp)/&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>k$/p/year</units>
			</aux>
			<aux name="sIIEeoROTA&lt;0">
				<eqn>-0.1</eqn>
				<units>1</units>
			</aux>
			<aux name="Rate of technological\nadvance">
				<eqn>(Basic_rate_of_technological_advance)*Reduction_in_ROTA_from_inequality*OWeoROTA</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Basic rate of technological advance">
				<eqn>0.026068209938352</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Reduction in ROTA\nfrom inequality">
				<eqn>MIN(1, 1+&quot;sIIEeoROTA&lt;0&quot;*(&quot;INEQUALITY_INDEX_(1980=1)&quot;/1-1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost of TAs in 2022">
				<eqn>9145</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Extra cost of TAs\nfrom 2022" access="output">
				<eqn>MAX(0, &quot;Cost_of_TAs_G$/y&quot;-Cost_of_TAs_in_2022)</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Extra cost of TAs\nas share of GDP">
				<eqn>Extra_cost_of_TAs_from_2022/GDP</eqn>
				<units>1</units>
			</aux>
			<aux name="Productivity loss from\nunprofitable activity">
				<eqn>Extra_cost_of_TAs_as_share_of_GDP*Fraction_unprofitable_activity_in_TAs</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction unprofitable\nactivity in TAs">
				<eqn>0.3</eqn>
				<units>1</units>
			</aux>
			<aux name="Indicated TFP" access="output">
				<eqn>TFP_including_effect_of_5TAs</eqn>
				<units>1</units>
			</aux>
			<aux name="TFP including\neffect of 5TAs">
				<doc>	was
		&quot;TFP excluding effect of 5TAs (1)&quot; * ( 1 - &quot;Reduction in TFP from less-cost-effective activities (1)&quot; )</doc>
				<eqn>TFP_excluding_effect_of_5TAs*(1-Reduction_in_TFP_from_unprofitable_activity)</eqn>
				<units>1</units>
			</aux>
			<aux name="OWeoROTA">
				<eqn>1+&quot;sOWeoTFP&lt;0&quot;*(Observed_warming_deg_C/Observed_warming_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="sOWeoTFP&lt;0">
				<doc>	220323 Set to - 0.1 to get TFP reduced by 10 % at +2.5 deg C. (dvs -4% per deg C)</doc>
				<eqn>-0.25</eqn>
				<units>1</units>
			</aux>
			<aux name="xExtra TA cost in 2100 (share of GDP)">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="xExtra cost of TAs\nas share of GDP">
				<eqn>&quot;xExtra_TA_cost_in_2022_(share_of_GDP)&quot;+RAMP((&quot;xExtra_TA_cost_in_2100_(share_of_GDP)&quot;-&quot;xExtra_TA_cost_in_2022_(share_of_GDP)&quot;)/time_for_xExtra, 2022, 2022+78)</eqn>
				<units>1</units>
			</aux>
			<aux name="xExtra TA cost in 2022 (share of GDP)">
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<flow name="Change in TFP">
				<eqn>Rate_of_technological_advance*TFP_excluding_effect_of_5TAs</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="sIPReoVPSS&gt;0">
				<doc>	Guess</doc>
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="TFP in 1980">
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="Public services\nper person k">
				<eqn>(Value_of_public_services_supplied/Population_Mp)/&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>k$/p/year</units>
			</aux>
			<aux name="Value of public\nservices supplied">
				<eqn>&quot;Govmnt_purchases_G$/y&quot;*Productivity_of_public_purchases</eqn>
				<units>G$/year</units>
			</aux>
			<aux name="Productivity of\npublic purchases">
				<eqn>MAX(0, 1+&quot;sIPReoVPSS&gt;0&quot;*LN(Infrastructure_purchases_ratio/Infrastructure_purchases_ratio_in_1980))</eqn>
				<units>1</units>
			</aux>
			<aux name="Infrastructure purchases\nratio in 1980">
				<eqn>1.2</eqn>
				<units>year</units>
			</aux>
			<aux name="Reduction in TFP from\nunprofitable activity">
				<eqn>SMTH1(Productivity_loss_from_unprofitable_activity, Investment_planning_time_y+Construction_time_PIS_y)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<aux name="Govmnt spending as share of GDP">
				<eqn>&quot;Govmnt_spending_G$/y&quot;/GDP</eqn>
				<units>1</units>
			</aux>
			<stock name="TFP excluding\neffect of 5TAs">
				<eqn>TFP_in_1980</eqn>
				<inflow>Change_in_TFP</inflow>
				<units>1</units>
			</stock>
			<aux name="Infrastructure\npurchases ratio">
				<doc>	Is te equivalent of the capital output ratio in the productive economy.
		Happens to be 6 years rather than 2.5</doc>
				<eqn>Capacity_PUS_Gcu/&quot;Govmnt_purchases_G$/y&quot;</eqn>
				<units>year</units>
			</aux>
			<aux name="Govmnt spending G$/y" access="input">
				<units>G$/year</units>
			</aux>
			<aux name="INEQUALITY INDEX (1980=1)" access="input">
				<units>1</units>
			</aux>
			<aux name="Observed warming deg C" access="input">
				<units>deg C</units>
			</aux>
			<aux name="Population Mp" access="input">
				<units>Mp</units>
			</aux>
			<aux name="Cost of TAs G$/y" access="input">
				<units>G$/year</units>
			</aux>
			<aux name="Investment planning time y" access="input">
				<units>year</units>
			</aux>
			<aux name="Construction time PIS y" access="input">
				<units>year</units>
			</aux>
			<stock name="Capacity PUS Gcu" access="input">
				<units>G$</units>
			</stock>
			<aux name="Govmnt purchases G$/y" access="input">
				<units>G$/year</units>
			</aux>
			<aux name="time for xExtra">
				<eqn>78</eqn>
				<units>year</units>
			</aux>
			<aux name="UNIT G/(M*k)" access="input">
				<eqn>1</eqn>
				<units>G/(M*k)</units>
			</aux>
			<aux name="GDP" access="input">
				<units>G$/Years</units>
			</aux>
			<aux name="Observed warming in 1980" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>deg C</units>
			</aux>
			<aux name="GDP 1" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>G$/Years</units>
			</aux>
		</variables>
