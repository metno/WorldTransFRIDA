	<model name="Population">
		<variables>
			<aux name="Desired no of children">
				<eqn>MIN(DNCmax,  ((DNCmin+(DNC_in_1980-DNCmin)*EXP(-DNCgamma*(Effective_GDP_per_person-GDP_per_person_in_1980)))*(1+DNCalfa*(Effective_GDP_per_person-GDP_per_person_in_1980)))*(1-Extra_fertility_reduction)*Fertility_multiplier)</eqn>
				<units>1</units>
			</aux>
			<aux name="DNCmin">
				<doc>	was 1.6, so 1.5
		211202 changed from 1.5 to 1.6
		211129 changed from 1.2 to compensate for change in age structure</doc>
				<eqn>1.5</eqn>
				<units>1</units>
			</aux>
			<aux name="DNC in 1980">
				<doc>	was 4.5, so 4.2
		211202 changed to 4.5 from 4.2
		211126 changed from 4.5 to get pop to 6000 in 2000
		DF in 1980 was 4.7 so 5.0 so 4.9 so 4.4, so 4.2, so 4.4</doc>
				<eqn>4.03409168318691</eqn>
				<units>1</units>
			</aux>
			<aux name="DNCgamma">
				<doc>	was 0.12, so 0.17
		211129 changed from 0.13 to 0.12 to compensate for new PUS and CLR
		211127 changed from 0.22 at the same time as changing initial values of age groups
		was 0.12, so 0.17, so 0.33, so 0.4, so 0.25, so 0.2, so 0.16</doc>
				<eqn>0.16</eqn>
				<units>(p*year)/(k$)</units>
			</aux>
			<aux name="Effective GDP per person" access="output">
				<eqn>SMTH1(GDP_per_person, Time_to_adapt_to_higher_income, GDP_per_person_in_1980)</eqn>
				<isee:delay_aux/>
				<units>k$/(p*year)</units>
			</aux>
			<aux name="GDP per person in 1980" access="output">
				<doc>	was 6.1</doc>
				<eqn>6.4</eqn>
				<units>k$/(p*year)</units>
			</aux>
			<aux name="DNCalfa">
				<doc>	was 0.02
		220113 Changed from 0 to 0.02 to avoid too rapid decline in pop towards 2100.
		Rate of decline of Minimum DF was 0.015, AND THEN 0.05 so 0.001, so 0.007</doc>
				<eqn>0.0214763598734662</eqn>
				<units>(p*year)/(k$)</units>
			</aux>
			<aux name="Fertility multiplier">
				<eqn>( IF &quot;SSP2_family_action_from_2022?&quot; &gt; 0 THEN ( IF TIME &gt; 2022 THEN 1+RAMP((Max_fertility_multiplier-1)/Time_for_multiplier, 2022, 2100) ELSE 1 ) ELSE 1 )</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost of extra fertility reduction (share of GDP)">
				<eqn>&quot;Cost_of_Max_fertility_reduction_(share_of_GDP)&quot;*Extra_fertility_reduction</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost of Max fertility reduction (share of GDP)">
				<eqn>0.01</eqn>
				<units>1</units>
			</aux>
			<aux name="Life expectancy">
				<eqn>((LEmax-(LEmax-LE_in_1980)*EXP(-LEgamma*(Effective_GDP_per_person-GDP_per_person_in_1980)))*(1+LEalfa*(Effective_GDP_per_person-GDP_per_person_in_1980)))*Warming_effect_on_life_expectancy*Life_expectancy_multipler</eqn>
				<units>year</units>
			</aux>
			<aux name="LEmax">
				<doc>	Maximum LE in 1980</doc>
				<eqn>85</eqn>
				<units>year</units>
			</aux>
			<aux name="LE in 1980">
				<doc>	was 64
		220113 changed from 67 to 62 to fit China
		LE in 1980</doc>
				<eqn>61.3689408612321</eqn>
				<units>year</units>
			</aux>
			<aux name="LEgamma">
				<doc>	Rate of increase in LE</doc>
				<eqn>0.09375</eqn>
				<units>1/(k$*p*year)</units>
			</aux>
			<aux name="LEalfa">
				<doc>	Rate of increase in max LE after 1980</doc>
				<eqn>0.001</eqn>
				<units>(p*year)/(k$)</units>
			</aux>
			<aux name="Warming effect on\nlife expectancy">
				<eqn>1+&quot;sOWeoLE&lt;0&quot;*(Observed_warming_deg_C/Observed_warming_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Life expectancy multipler">
				<eqn>( IF &quot;SSP2_family_action_from_2022?&quot; &gt; 0 THEN ( IF TIME &gt; 2022 THEN 1+RAMP((Max_life_expectancy_multiplier-1)/Time_for_multiplier, 2022, 2100) ELSE 1 ) ELSE 1 )</eqn>
				<units>1</units>
			</aux>
			<aux name="SSP2 family action\nfrom 2022?">
				<doc>	1 means action as per SSP2. 0 means no action</doc>
				<eqn>1</eqn>
				<units>1</units>
			</aux>
			<aux name="Max fertility multiplier">
				<doc>	220224: changed from 1 to 1.6 to match SSP 2</doc>
				<eqn>1.6</eqn>
				<units>1</units>
			</aux>
			<aux name="Max life expectancy\nmultiplier">
				<doc>	to match SSP2 assumption of LE up 2 years per decade</doc>
				<eqn>1.1</eqn>
				<units>1</units>
			</aux>
			<aux name="GDP per person" access="output">
				<eqn>(GDP/Population)/&quot;UNIT_G/(M*k)&quot;</eqn>
				<units>k$/(p*year)</units>
			</aux>
			<aux name="Extra fertility reduction">
				<eqn>RAMP(Goal_for_extra_fertility_reduction/Introduction_period_for_policy_y, 2022, 2022+Introduction_period_for_policy_y)</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for extra\nfertility reduction">
				<doc>	0 in TLTL and 0.2 in GL</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra pension age">
				<eqn>RAMP((Goal_for_extra_pension_age-Extra_pension_age_in_2022)/Introduction_period_for_policy_y, 2022, 22022+Introduction_period_for_policy_y)</eqn>
				<units>year</units>
			</aux>
			<aux name="Goal for extra pension age">
				<doc>	Can be set to 5, if one want to lift the pension age from 82 to 89 in 2100.</doc>
				<eqn>0</eqn>
				<units>year</units>
			</aux>
			<aux name="Extra pension age in 2022">
				<eqn>0</eqn>
				<units>year</units>
			</aux>
			<aux name="Population" access="output">
				<eqn>&quot;Aged_0-20_years&quot;+&quot;Aged_20-40_years&quot;+&quot;Aged_40-60_years&quot;+&quot;Aged_60_+_years&quot;</eqn>
				<units>Mp</units>
			</aux>
			<stock name="Aged 20-40 years">
				<doc>	was 1283</doc>
				<eqn>&quot;Aged_20-40_in_1980&quot;</eqn>
				<inflow>Passing_20</inflow>
				<outflow>Passing_40</outflow>
				<outflow>&quot;excess_deaths_20-40_years&quot;</outflow>
				<units>Mp</units>
			</stock>
			<aux name="Fraction women">
				<eqn>0.5</eqn>
				<units>1</units>
			</aux>
			<aux name="Fertile period">
				<eqn>20</eqn>
				<units>year</units>
			</aux>
			<aux name="sOWeoLE&lt;0">
				<doc>	Set to -0.02 means a reduction in LE by 2.5 years in 2100 at +2.5 deg C, ie 1 years per deg C
		NN in SDR 2021 used &quot;Fractional increase in death rates per additional degree = 0.02&quot;.
		means a reduction in LE of 2 years when temp increases 1 deg C.</doc>
				<eqn>-0.01</eqn>
				<units>1</units>
			</aux>
			<aux name="Aged 20-pension age" access="output">
				<eqn>&quot;Aged_20-40_years&quot;+&quot;Aged_40-60_years&quot;+&quot;Aged_60_+_years&quot;</eqn>
				<units>Mp</units>
			</aux>
			<aux name="Observed fertility">
				<doc>	220207 changed from &quot;As math: Observed fertility 1&quot;
		220114 changed from Desired no of children 1 * &quot;Fraction achieving desired family size (1)&quot;
		210821 changed from: IF THEN ELSE(Time&gt;&quot;Fertility = 2.5 introduction time y&quot;, 2.5 , Desired no of children 1)</doc>
				<eqn>Desired_no_of_children*Fraction_achieving_desired_family_size</eqn>
				<units>1</units>
			</aux>
			<aux name="Fraction achieving\ndesired family size">
				<eqn>0.8</eqn>
				<units>1</units>
			</aux>
			<aux name="Passing 40 in 1980">
				<doc>	was 13</doc>
				<eqn>64</eqn>
				<units>Mp/year</units>
			</aux>
			<aux name="Passing 60 in 1980">
				<doc>	was 8</doc>
				<eqn>38</eqn>
				<units>Mp/year</units>
			</aux>
			<flow name="Passing 60">
				<eqn>DELAYN(Passing_40,20,10,Passing_60_in_1980)-&quot;excess_deaths_40-60_years&quot;</eqn>
				<units>Mp/Year</units>
			</flow>
			<flow name="Passing 40">
				<eqn>DELAYN(Passing_20,20,10,Passing_40_in_1980)-&quot;excess_deaths_20-40_years&quot;</eqn>
				<units>Mp/Year</units>
			</flow>
			<flow name="Deaths">
				<doc>	211111: Changed from IF THEN ELSE(LE at 60 y &gt; 0, SMOOTH3(&quot;Aged 60 + Mp&quot; / LE at 60 y, 20), 0)</doc>
				<eqn>DELAYN(Passing_60,LE_at_60,10,Dying_in_1980)-&quot;excess_deaths_60_+&quot;</eqn>
				<units>Mp/Year</units>
			</flow>
			<aux name="LE at 60">
				<eqn>Life_expectancy-60</eqn>
				<units>year</units>
			</aux>
			<aux name="Dying in 1980">
				<doc>	was 6.3</doc>
				<eqn>30</eqn>
				<units>Mp/year</units>
			</aux>
			<aux name="Aged 40-60 in 1980">
				<doc>	was 174</doc>
				<eqn>670.581566238008</eqn>
				<units>Mp</units>
			</aux>
			<stock name="Aged 60 + years">
				<doc>	was 382</doc>
				<eqn>&quot;Aged_60+_in_1980&quot;</eqn>
				<inflow>Passing_60</inflow>
				<outflow>Deaths</outflow>
				<outflow>&quot;excess_deaths_60_+&quot;</outflow>
				<units>Mp</units>
			</stock>
			<aux name="Aged 60+ in 1980">
				<doc>	was 86</doc>
				<eqn>411.223185995747</eqn>
				<units>Mp</units>
			</aux>
			<flow name="Passing 20">
				<eqn>DELAYN(Births,20,10,Passing_20_in_1980)-&quot;excess_deaths_0-20_years&quot;</eqn>
				<units>Mp/Year</units>
			</flow>
			<stock name="Aged 40-60 years">
				<doc>	was 768</doc>
				<eqn>&quot;Aged_40-60_in_1980&quot;</eqn>
				<inflow>Passing_40</inflow>
				<outflow>Passing_60</outflow>
				<outflow>&quot;excess_deaths_40-60_years&quot;</outflow>
				<units>Mp</units>
			</stock>
			<aux name="Passing 20 in 1980">
				<doc>	was 21</doc>
				<eqn>100</eqn>
				<units>Mp/year</units>
			</aux>
			<flow name="Births">
				<eqn>&quot;Aged_20-40_years&quot;*Fraction_women*(Observed_fertility/Fertile_period)</eqn>
				<units>Mp/Year</units>
			</flow>
			<aux name="Pension age">
				<eqn>( IF Life_expectancy &lt; LE_in_1980 THEN Pension_age_in_1980 ELSE Pension_age_in_1980+&quot;sLEeoPa&gt;0&quot;*(Life_expectancy+Extra_pension_age-LE_in_1980) )</eqn>
				<units>year</units>
			</aux>
			<aux name="Pension age in 1980">
				<doc>	220113 Changed back from 67 to 62, assuming that people under 20 also participate in the workforce.
		220109 changed from 62 to 67 to get sufficient labour force (ie over 500 Mp)</doc>
				<eqn>62</eqn>
				<units>year</units>
			</aux>
			<aux name="sLEeoPa&gt;0">
				<doc>	was 0.65, so 0.75, so 0.3</doc>
				<eqn>0.75</eqn>
				<units>1</units>
			</aux>
			<aux name="Time to adapt to\nhigher income">
				<doc>	was 3</doc>
				<eqn>10</eqn>
				<units>year</units>
			</aux>
			<aux name="Population growth rate">
				<eqn>Birth_rate-Death_rate</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Birth rate">
				<eqn>Births/Population</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Death rate">
				<eqn>(Deaths+&quot;excess_deaths_0-20_years&quot;+&quot;excess_deaths_20-40_years&quot;+&quot;excess_deaths_40-60_years&quot;+&quot;excess_deaths_60_+&quot;)/Population</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Aged 0-20 in 1980">
				<doc>	was 2050, so 2170, so 491
		211202 changed to 2050 from 2170
		211127 changed from 2050
		was 2025</doc>
				<eqn>1999.67512821747</eqn>
				<units>Mp</units>
			</aux>
			<stock name="Aged 0-20 years">
				<doc>	was 2025</doc>
				<eqn>&quot;Aged_0-20_in_1980&quot;</eqn>
				<inflow>Births</inflow>
				<outflow>Passing_20</outflow>
				<outflow>&quot;excess_deaths_0-20_years&quot;</outflow>
				<units>Mp</units>
