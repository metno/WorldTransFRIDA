	<model name="Labour_market">
		<variables>
			<aux name="GDP per person in 1980" access="output">
				<doc>	was 6.1</doc>
				<eqn>INIT(&quot;GDP_per_person_k$/p/y&quot;)</eqn>
				<units>k$/(p*year)</units>
			</aux>
			<aux name="Normal LPR">
				<eqn>(Normal_LPR_in_1980*(1+&quot;sWSOeoLPR&gt;0&quot;*(Worker_share_of_output/WSO_in_1980-1)))+Extra_normal_LPR_from_2022</eqn>
				<units>1</units>
			</aux>
			<aux name="Normal LPR in 1980">
				<doc>	was 0.7</doc>
				<eqn>0.773059967396461</eqn>
				<units>1</units>
			</aux>
			<aux name="sWSOeoLPR&gt;0">
				<doc>	was 1, so 0, so 0.5</doc>
				<eqn>0.2</eqn>
				<units>1</units>
			</aux>
			<aux name="WSO in 1980" access="output">
				<eqn>0.542482308163222</eqn>
				<units>1</units>
			</aux>
			<aux name="Extra normal LPR from 2022">
				<eqn>RAMP(Goal_for_extra_normal_LPR/Introduction_period_for_policy_y, 2022, 2022+Introduction_period_for_policy_y)</eqn>
				<units>1</units>
			</aux>
			<aux name="Goal for extra normal LPR">
				<doc>	was 0.05
		remeber it takes a lot of time to increase LPR</doc>
				<eqn>0</eqn>
				<units>1</units>
			</aux>
			<aux name="Unemployment perception time">
				<doc>	was 2, so 1, so 0.8, so 0.7, so 0.9,so 0.8</doc>
				<eqn>0.25</eqn>
				<units>year</units>
			</aux>
			<aux name="Hiring/firing delay">
				<doc>	was 1, so 0.8, so 0.7, so 0.9, so 0.8,</doc>
				<eqn>0.25</eqn>
				<units>year</units>
			</aux>
			<stock name="Worker share of output" access="output">
				<eqn>WSO_in_1980</eqn>
				<inflow>Change_in_wso</inflow>
				<outflow>&quot;Long-term_erosion_of_wso&quot;</outflow>
				<units>1</units>
			</stock>
			<aux name="Labour participation rate" access="output">
				<eqn>SMTH1(Indicated_labour_participation_rate, &quot;Time_to_enter/leave_labor_market&quot;)</eqn>
				<isee:delay_aux/>
				<units>1</units>
			</aux>
			<stock name="Wage rate">
				<eqn>Wage_rate_in_1980</eqn>
				<inflow>Change_in_wage_rate</inflow>
				<units>$/h</units>
			</stock>
			<aux name="Average hours worked">
				<eqn>Normal_hours_worked/&quot;Persons_per_full-time_job&quot;</eqn>
				<units>kh/(p*year)</units>
			</aux>
			<aux name="Labour use" access="output">
				<eqn>(Workforce*Average_hours_worked)*&quot;UNIT_Gh/Mkh&quot;</eqn>
				<units>Gh/year</units>
			</aux>
			<aux name="Labour use in 1980" access="output">
				<eqn>(workforce_in_1980*&quot;Average_hours_worked_in_1980_kh/y&quot;)*&quot;UNIT_Gh/Mkh&quot;</eqn>
				<units>Gh/year</units>
			</aux>
			<aux name="Average hours worked in 1980 kh/y">
				<eqn>Normal_hours_worked_in_1980/&quot;Persons_per_full-time_job_in_1980&quot;</eqn>
				<units>kh/(p*year)</units>
			</aux>
			<aux name="wage rate effect\non new workers">
				<eqn>sWReoUR*(Wage_rate/Wage_rate_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="sWReoUR">
				<doc>	was 0.6, so 1.2, so 1, so 1.1, so 1</doc>
				<eqn>-0.17</eqn>
				<units>1</units>
			</aux>
			<aux name="Normal hours worked\nin 1980">
				<eqn>2</eqn>
				<units>kh/(ftj*year)</units>
			</aux>
			<aux name="Persons per full-time\njob in 1980">
				<eqn>1</eqn>
				<units>p/ftj</units>
			</aux>
			<aux name="Normal hours worked">
				<eqn>SMTH1(Normal_hours_worked_in_1980*Hours_worked_mult_from_GDPpp, Time_to_adjust_hours_worked, Normal_hours_worked_in_1980)</eqn>
				<isee:delay_aux/>
				<units>kh/(ftj*year)</units>
			</aux>
			<aux name="Persons per full-time job">
				<eqn>1</eqn>
				<units>p/ftj</units>
			</aux>
			<aux name="Labour productivity">
				<eqn>GDP/Labour_use</eqn>
				<units>$/h</units>
			</aux>
			<aux name="Labour productivity\nin 1980">
				<eqn>(&quot;Optimal_output_in_1980_Gu/y&quot;*&quot;Cost_per_unit_in_1980_$/u&quot;)/Labour_use_in_1980</eqn>
				<units>$/h</units>
			</aux>
			<flow name="Change in\nwage rate">
				<eqn>Wage_rate*&quot;ROC_in_WSO_-_Table&quot;</eqn>
				<units>$/h/Year</units>
			</flow>
			<aux name="ROC in WSO - Table">
				<eqn>Perceived_unemployment_rate/Acceptable_unemployment_rate</eqn>
				<gf>
					<yscale min="-0.01" max="0.06"/>
					<xpts>0,0.5,1,1.5,2</xpts>
					<ypts>0.06,0.02,0,-0.007,-0.01</ypts>
				</gf>
				<units>1/year</units>
			</aux>
			<aux name="Wage share" access="output">
				<eqn>Wage_rate/Labour_productivity</eqn>
				<units>1</units>
			</aux>
			<aux name="Wage rate in 1980">
				<eqn>Labour_productivity_in_1980*WSO_in_1980</eqn>
				<units>$/h</units>
			</aux>
			<aux name="Indicated labour\nparticipation rate">
				<eqn>Normal_LPR-Perceived_surplus_workforce</eqn>
				<units>1</units>
			</aux>
			<aux name="Time to enter/leave\nlabor market">
				<eqn>5</eqn>
				<units>year</units>
			</aux>
			<aux name="Acceptable\nunemployment rate">
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="sTIeoNHW&lt;0">
				<doc>	NWH falls linerly from 2000 h/y at 6 k$/p/y to 1500 h/y at 48 k$/p/y.
		Means sTeoNHW&lt;0=-0.03</doc>
				<eqn>-0.03</eqn>
				<units>1</units>
			</aux>
			<aux name="Hours worked mult\nfrom GDPpp">
				<doc>	Falls linerly from 2000 h/y at 6 k$/p/y to 1500 h/y at 48 k$/p/y.
		Means sTeoNHW&lt;0=-0.03</doc>
				<eqn>1+&quot;sTIeoNHW&lt;0&quot;*(&quot;GDP_per_person_k$/p/y&quot;/GDP_per_person_in_1980-1)</eqn>
				<units>1</units>
			</aux>
			<aux name="Time to adjust hours worked">
				<eqn>5</eqn>
				<units>year</units>
			</aux>
			<aux name="Real wage erosion rate">
				<doc>	was 0.015, so 0.01,</doc>
				<eqn>0.000233860650819518</eqn>
				<units>1/year</units>
			</aux>
			<aux name="Working age population">
				<eqn>&quot;Aged_20-pension_age_Mp&quot;</eqn>
				<units>Mp</units>
			</aux>
			<aux name="Available workforce">
				<eqn>Working_age_population*Labour_participation_rate</eqn>
				<units>Mp</units>
			</aux>
			<flow name="Long-term erosion of wso">
				<eqn>Worker_share_of_output*Real_wage_erosion_rate</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="sPUNeoLPR&gt;0">
				<doc>	was 0.5, so 0</doc>
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<flow name="Change in wso">
				<eqn>Worker_share_of_output*&quot;ROC_in_WSO_-_Table&quot;</eqn>
				<units>Per Year</units>
			</flow>
			<aux name="Perceived surplus workforce">
				<eqn>Acceptable_unemployment_rate*(1+&quot;sPUNeoLPR&gt;0&quot;*(Perceived_unemployment_rate/Acceptable_unemployment_rate-1))</eqn>
				<units>1</units>
			</aux>
			<aux name="Cost per unit in 1980 $/u" access="input">
				<units>$/$</units>
			</aux>
			<aux name="Introduction period for policy y" access="input">
				<units>year</units>
			</aux>
			<aux name="GDP per person k$/p/y" access="input">
				<units>k$/(p*year)</units>
			</aux>
			<aux name="Optimal output in 1980 Gu/y" access="input">
				<units>G$/Years</units>
			</aux>
			<aux name="Capacity Gcu" access="input">
				<units>G$</units>
			</aux>
			<aux name="Aged 20-pension age Mp" access="input">
				<units>Mp</units>
			</aux>
			<aux name="UNIT Gh/Mkh">
				<eqn>1</eqn>
				<units>Gh/Mkh</units>
			</aux>
			<aux name="GDP" access="input">
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>G$/Years</units>
			</aux>
			<stock name="Unemployment rate" access="output">
				<eqn>Unemployment_rate_in_1980</eqn>
				<inflow>change_in_unemployment_rate</inflow>
				<units>1</units>
			</stock>
			<aux name="Workforce" access="output">
				<eqn>Available_workforce*(1-Unemployment_rate)</eqn>
