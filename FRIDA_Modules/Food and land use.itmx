					<dim name="Run"/>
				</dimensions>
				<eqn>crop_demand_for_direct_consumption + crop_demand_from_livestock</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="desired crop reserve capacity" access="output">
				<doc>Calibration Parameter: forest land &amp; CO2 emissions from land cover change &amp; agricultural land &amp; crop production &amp; fertilizer use</doc>
				<eqn>0.05</eqn>
				<units>1</units>
			</aux>
			<aux name="GDP Per Capita" access="output" throughput="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<units>b$/Mp/year</units>
			</aux>
			<aux name="elasticity of crop demand\nfor direct consumption\nto GDP per person">
				<doc>Calibration Parameter: fertilizer use &amp; crop production</doc>
				<eqn>0.36732616349464</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="Population" access="output" throughput="true">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>{Enter equation for use when not hooked up to other models}</eqn>
				<units>Mp</units>
			</aux>
			<aux name="crop demand\nfor direct consumption\nper capita 1980">
				<doc>Calibration Parameter: fertilizer use &amp; crop production</doc>
				<eqn>0.814870531313227</eqn>
				<units>Mtcrop/Mp/year</units>
			</aux>
			<aux name="crop demand\nfor direct consumption\nper capita">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>IF switch_crop_demand_for_direct_consumption_per_Mp_policy = 0 OR TIME &lt; policy_start_year THEN
((GDP_Per_Capita/INIT(GDP_Per_Capita))^elasticity_of_crop_demand_for_direct_consumption_to_GDP_per_person)*crop_demand_for_direct_consumption_per_capita_1980
ELSE
crop_demand_for_direct_consumption_per_Mp_policy</eqn>
				<units>Mtcrop/Mp/year</units>
			</aux>
			<aux name="livestock demand\nper capita">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>IF switch_livestock_demand_per_Mp_policy = 0 OR TIME &lt; policy_start_year THEN
((GDP_Per_Capita/INIT(GDP_Per_Capita))^elasticity_of_livestock_demand_to_GDP_per_person)*livestock_demand_per_capita_1980 ELSE
livestock_demand_per_Mp_policy</eqn>
				<units>Mkg/Mp/year</units>
			</aux>
			<aux name="elasticity of livestock demand\nto GDP per person">
				<doc>Calibration Parameter: fertilizer use &amp; crop production</doc>
				<eqn>0.338178085358638</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="livestock demand\nper capita 1980">
				<doc>Calibration Parameter: fertilizer use &amp; crop production</doc>
				<eqn>31.538896085565</eqn>
				<units>Mkg/Mp/year</units>
			</aux>
			<aux name="livestock demand" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Population*livestock_demand_per_capita</eqn>
				<units>MKg/Year</units>
			</aux>
			<aux name="crop demand from\nlivestock">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>livestock_demand*crops_required_per_MKg_of_livestock_demand</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="crops required\nper MKg of livestock\ndemand in 1980">
				<eqn>0.0105095634923097</eqn>
				<units>Mtcrop/MKg</units>
			</aux>
			<aux name="effect of livestock\nintensification on crop\ndemand">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Livestock_Yield/INIT(Livestock_Yield) )^elasticity_of_crops_required_for_livestock_to_livestock_yield</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="crops required\nper MKg of livestock\ndemand">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>crops_required_per_MKg_of_livestock_demand_in_1980*effect_of_livestock_intensification_on_crop_demand</eqn>
				<units>Mtcrop/MKg</units>
			</aux>
			<aux name="elasticity of\ncrops required for livestock\nto livestock yield">
				<eqn>0.240247075848343</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="crop demand\nfor direct consumption">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>crop_demand_for_direct_consumption_per_capita*Population</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="perceived crop\ndemand" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>SMTH1(crop_demand*(1+desired_crop_reserve_capacity), time_to_perceive_crop_demand)</eqn>
				<isee:delay_aux/>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="time to perceive\ncrop demand">
