	<model name="Livestock">
		<variables>
			<aux name="livestock balance" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>livestock_production/livestock_demand</eqn>
				<units>1</units>
			</aux>
			<aux name="effect of livestock balance\non grazing land" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>1+sensitivity_of_effect_of_livestock_balance_on_grassland_expansion_rate*(livestock_balance_with_reserves-1)</eqn>
				<units>dmnl</units>
			</aux>
			<flow name="livestock\nyield intensification">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>normal_rate_of_livestock_intensification*Livestock_Yield*effect_of_livestock_balance_on_yield_intensification</eqn>
