	<model name="Food_and_land_use">
		<variables>
			<aux name="Albedo forest land">
				<eqn>0.15</eqn>
				<units>%</units>
			</aux>
			<aux name="Albedo cropland">
				<eqn>0.22</eqn>
				<units>%</units>
			</aux>
			<aux name="Albedo grassland">
				<eqn>0.21</eqn>
				<units>%</units>
			</aux>
			<aux name="Albedo">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Mature_Forest * Albedo_forest_land + Grassland * Albedo_grassland + Cropland * Albedo_cropland + Recovering_Forest *albedo_recovering_forest )
/ (Mature_Forest + Cropland + Grassland + Recovering_Forest)</eqn>
				<units>%</units>
			</aux>
			<aux name="albedo recovering forest">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(Albedo_grassland+Albedo_forest_land)/2</eqn>
				<units>%</units>
			</aux>
			<isee:placeholder name="calibration data">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>Land_Use.Forest_Land + Land_Use.Cropland + Crop.crop_production + Livestock.livestock_production + Freshwater.total_water_withdrawal + Land_Nutrients.fertilizer_use + Land_Nutrients.manure_fertilizer_use + Land_Nutrients.man_made_fertilizer_use + Food_and_land_use.CO2_emissions + Food_and_land_use.N2O_emissions + Food_and_land_use.CH4_Emissions + Crop.crop_balance + Livestock.livestock_balance + Freshwater.fraction_of_water_withdrawal_for_agriculture + Food_Demand.fraction_of_crops_for_livestock + Freshwater.available_water_supply + Soil.soil_carbon_loss_cropland</eqn>
			</isee:placeholder>
			<aux name="CO2 emissions by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(CO2_emissions,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>MtCO2/Year</units>
			</aux>
			<aux name="N2O emissions by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(N2O_emissions,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>KtN2O/Year</units>
			</aux>
			<aux name="CH4 Emissions by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(CH4_Emissions,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>MtCH4/Year</units>
			</aux>
			<aux name="SO2 Emissions by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(SO2_Emissions,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>MtSO2/Year</units>
			</aux>
			<aux name="Forest Land by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(Forest_Land,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>Mha</units>
			</aux>
			<aux name="crop production by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(crop_production,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>Mtcrop/year</units>
			</aux>
			<aux name="Cropland by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(Cropland,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>Mha</units>
			</aux>
			<aux name="total\nwater\nwithdrawal by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(total_water_withdrawal,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
				<units>m^3/year</units>
			</aux>
			<aux name="livestock production by PCTILE">
				<dimensions>
					<dim name="PCTILE"/>
				</dimensions>
				<eqn>PERCENTILE(production,  PCTILE5(PCTILE), PCTILE5(PCTILE-1))</eqn>
