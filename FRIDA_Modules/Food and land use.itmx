	<model name="Soil">
		<variables>
			<aux name="cropland soil carbon 1980">
				<eqn>161.0314 { LPJmL}</eqn>
				<units>GtC</units>
			</aux>
			<flow name="litter input fast soil carbon">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>litter_to_soil_carbon*(1-litter_input_share_slow_soil_carbon)</eqn>
				<non_negative/>
				<units>GtC/Year</units>
			</flow>
			<aux name="natural decay rate litter carbon">
				<eqn>0.7</eqn>
				<units>dmnl</units>
			</aux>
			<flow name="fast soil carbon emissions">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>fast_soil_carbon_cropland*decay_rate_fast_soil_carbon</eqn>
				<non_negative/>
				<units>GtC/Year</units>
			</flow>
			<aux name="slow soil carbon historic share">
				<eqn>0.35</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="litter input share slow soil\ncarbon">
				<eqn>0.015</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="natural decay rate fast soil carbon">
				<eqn>0.025</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="climate response term">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>MAX(0, 
climate_response_1980+(1-INIT(Surface_Temperature_Anomaly)/
Surface_Temperature_Anomaly)*climate_response_sensitivity
)</eqn>
				<units>dmnl</units>
			</aux>
			<stock name="fast soil carbon cropland">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>cropland_soil_carbon_1980*(1-slow_soil_carbon_historic_share)</eqn>
				<inflow>litter_input_fast_soil_carbon</inflow>
				<outflow>fast_soil_carbon_emissions</outflow>
				<units>GtC</units>
			</stock>
			<aux name="climate response 1980">
				<eqn>0.3</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="soil carbon cropland" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>fast_soil_carbon_cropland+slow_soil_carbon_cropland</eqn>
				<units>GtC</units>
			</aux>
			<aux name="decay rate fast soil carbon">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(1-EXP(-(natural_decay_rate_fast_soil_carbon*climate_response_term)))</eqn>
				<units>1/year</units>
			</aux>
			<aux name="soil carbon emissions GtC" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>MAX((fast_soil_carbon_emissions-litter_input_fast_soil_carbon) + (slow_soil_carbon_emissions-litter_input_slow_soil_carbon),  0)</eqn>
				<units>GtC/Year</units>
			</aux>
			<flow name="litter input slow soil carbon">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>litter_to_soil_carbon*litter_input_share_slow_soil_carbon</eqn>
				<non_negative/>
				<units>GtC/Year</units>
			</flow>
			<flow name="slow soil carbon emissions">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>slow_soil_carbon_cropland*decay_rate_slow_soil_carbon</eqn>
				<non_negative/>
				<units>GtC/Year</units>
			</flow>
			<stock name="slow soil carbon cropland">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>cropland_soil_carbon_1980*slow_soil_carbon_historic_share</eqn>
				<inflow>litter_input_slow_soil_carbon</inflow>
				<outflow>slow_soil_carbon_emissions</outflow>
				<units>GtC</units>
			</stock>
			<aux name="natural decay rate slow soil carbon">
				<eqn>0.001</eqn>
				<units>dmnl/year</units>
			</aux>
			<aux name="decay rate slow\nsoil carbon">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(1-EXP(-(natural_decay_rate_slow_soil_carbon*climate_response_term)))</eqn>
				<units>1/year</units>
			</aux>
			<aux name="decay rate litter carbon">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>(1-EXP(-(natural_decay_rate_litter_carbon*climate_response_term*litter_decay_factor)))</eqn>
				<units>dmnl</units>
			</aux>
			<aux name="soil carbon loss cropland" access="output">
				<dimensions>
					<dim name="Run"/>
				</dimensions>
				<eqn>cropland_soil_carbon_1980 - soil_carbon_cropland</eqn>
				<units>GtC</units>
